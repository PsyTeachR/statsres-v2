	institution = {PsyArXiv},
	author = {Wong, Tsz Keung and Kiers, Henk and Tendeiro, Jorge},
	month = aug,
	year = {2021},
	doi = {10.31234/osf.io/86p4k},
	note = {type: article},
	keywords = {Social and Behavioral Sciences, Quantitative Methods, Statistical Methods, Bayes Factor, Null Hypothesis Bayesian Testing, Questionable Reporting and Interpreting Practice, Statistical test},
	file = {Full Text PDF:C\:\\Users\\jamesb\\Zotero\\storage\\E6CNXA25\\Wong et al. - 2021 - On the Potential Mismatch between the Function of .pdf:application/pdf},
