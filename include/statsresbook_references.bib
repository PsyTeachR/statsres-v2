@article{dawtry_why_2015,
	title = {Why {Wealthier} {People} {Think} {People} {Are} {Wealthier}, and {Why} {It} {Matters}: {From} {Social} {Sampling} to {Attitudes} to {Redistribution}},
	volume = {26},
